{
	"schemaVersion": 1,
	"id": "zerocraft",
	"version": "${version}",
	"name": "ZeroCraft",
	"description": "Powered by Zero-Alignment \n Notice: This mod is just a fan art and dose not have any association to ZeroERA official. \n ZeroCraft is a mod which implements various features from ZeroERA and a few gadgets into Minecraft.",
	"authors": [
		"redColmula55"
	],
	"contact": {
		"homepage": "https://github.com/redColmula55",
		"sources": "https://github.com/redColmula55/zerocraft",
		"issues": "https://github.com/redColmula55/zerocraft/issues"
	},
	"license": "GPL-3.0 & CC-BY-NC-SA 4.0",
	"icon": "assets/zerocraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"rc55.mc.zerocraft.ZeroCraft"
		],
		"client": [
			"rc55.mc.zerocraft.ZeroCraftClient"
		],
		"fabric-datagen": [
			"rc55.mc.zerocraft.ZeroCraftDataGenerator"
		]
	},
	"mixins": [
		"zerocraft.mixins.json"
	],
	"accessWidener": "zerocraft.accesswidener",
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/class_3611": ["rc55/mc/zerocraft/fluid/FluidTemperature"]
		}
	},
	"suggests": {
		"another-mod": "*"
	}
}